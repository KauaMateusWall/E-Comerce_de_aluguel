                    </columns>
                </TableView>
                <Label layoutX="126.0" layoutY="45.0" prefHeight="64.0" prefWidth="307.0" text="Meus Produtos ">
                    <font>
                        <Font size="43.0" />
                    </font>
                </Label>
                <Button layoutX="245.0" layoutY="391.0" mnemonicParsing="false" text="Voltar" />
            </children>
        </AnchorPane>
    </children>
</VBox>
</Scene>