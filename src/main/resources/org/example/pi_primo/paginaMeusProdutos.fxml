<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.Scene?>

<Scene fx:id="mainScene" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.pi_primo.service.TelaMeusProdutos">
    <VBox prefHeight="448.0" prefWidth="559.0">
        <children>
            <AnchorPane prefHeight="499.0" prefWidth="559.0">
                <children>
                    <TableView fx:id="TabelaListaProduto" layoutY="137.0" prefHeight="242.0" prefWidth="559.0" onMouseClicked="#handleProductSelection">
                        <columns>
                            <TableColumn fx:id="colunaId" prefWidth="75.0" text="ID" />
                            <TableColumn fx:id="colunaNome" prefWidth="75.0" text="Nome" />
                            <TableColumn fx:id="colunaPreco" prefWidth="75.0" text="Preço" />
                            <TableColumn fx:id="colunaCategoria" prefWidth="75.0" text="Categoria" />
                            <TableColumn fx:id="colunaQuantidadeDeEmprestimos" prefWidth="188.0" text="Quantidade de Empréstimos" />
                            <TableColumn fx:id="colunaSituacao" prefWidth="70.0" text="Situação" />
                        </columns>
                    </TableView>
                    <Label layoutX="126.0" layoutY="45.0" prefHeight="64.0" prefWidth="307.0" text="Meus Produtos">
                        <font>
                            <Font size="43.0" />
                        </font>
                    </Label>
                    <Button layoutX="245.0" layoutY="391.0" mnemonicParsing="false" text="Voltar" onAction="#voltarTelaAnterior" />
                </children>
            </AnchorPane>
        </children>
    </VBox>
</Scene>
